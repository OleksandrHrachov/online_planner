{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/online_planner/(.*)",
      "dest": "dist/index.js",
      "methods": ["GET", "POST", "DELETE", "PATCH"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
